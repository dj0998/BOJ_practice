import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);

   int T = sc.nextInt();

   for(int i = 0; i<T; i++) {
     int H = sc.nextInt();
     int W = sc.nextInt();
     int N = sc.nextInt();
     int q = N/H;
     int r = N%H;
     
     if(r == 0) System.out.println(H*100+q);
     else System.out.println(r*100+q+1);
     }
  
  } 
}
