import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);

    int count = 0;
    int num = sc.nextInt();
    int[] car = new int[5];
    
    for(int i = 0; i < 5; i++){
      car[i] = sc.nextInt();
      if (car[i] == num) count++;
    }
    System.out.println(count);
  } 
}
