import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);
    
    String a = sc.next();
    String b = sc.next();
    String c = sc.next();
    String d = sc.next();

    String sum_ab = a + b;
    String sum_cd = c + d;
    
    System.out.println(Long.parseLong(sum_ab) + Long.parseLong(sum_cd));
  } 
}
