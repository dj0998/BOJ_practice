import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);
    
   while(true){
     String arr = sc.nextLine();
     
     if(arr.equals("END"))
       break;
     for (int i = arr.length()-1; i >= 0; i--) System.out.print(arr.charAt(i));
     System.out.println(" ");
   } 
    
  } 
}
