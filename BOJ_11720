import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    
  Scanner sc = new Scanner(System.in);

    int sum = 0;
    int num = sc.nextInt();
    String a = sc.next();
    
		for (int i = 0; i < num; i++) {
      
			sum += Character.getNumericValue(a.charAt(i));
      
		}
  System.out.println(sum);
	}
}
