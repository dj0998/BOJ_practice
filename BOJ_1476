import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);

    int e = sc.nextInt();
    int s = sc.nextInt();
    int m = sc.nextInt();
    int count = e;

    while((count - e) % 15 != 0 || (count - s) % 28 != 0 || (count - m) % 19 != 0) count += 15;
    System.out.println(count);
  }
}
