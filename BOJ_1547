import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);

    int num = sc.nextInt();
    int ball = 1;
  
    for(int i = 0; i < num; i++) {
      int a = sc.nextInt();
      int b = sc.nextInt();
      if(a == ball) ball = b;
      else if(b == ball) ball = a;
    }
    System.out.println(ball);
  } 
}
