import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);
    
    int num = sc.nextInt();
    int sum = 0;

    for(int i = 0; i < num; i++) {
      int p =sc.nextInt();
      sum += p;
    }
    System.out.println(sum-num+1);
  } 
}
