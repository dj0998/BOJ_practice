import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);
    
    int n = sc.nextInt();
    int sum = 1;
    int count = 1;
    
    if (n == 1) {
      System.out.println(1);
    }

    else {
      while (sum < n) {
        sum = sum + (6 * count);
        count++;
      }
    }
    System.out.println(count);
  } 
}
