import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);

    int t = sc.nextInt();

    for(int i = 0; i < t; i++) {
      int n = sc.nextInt();
      StringBuffer str = new StringBuffer(sc.next());
      str = str.deleteCharAt(n-1);
      System.out.println(str);
    }
  }  
}
