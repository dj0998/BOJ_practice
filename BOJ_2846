import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);

    int n = sc.nextInt();
    int[] arr = new int[n];
    int max = 0;
    int sum = 0;

    for(int i = 0; i < n; i++) {
      arr[i]= sc.nextInt();
    }
    
    for(int i = 0; i < n-1; i++) {
      if(arr[i] < arr[i+1]) sum += arr[i+1] - arr[i];
      else sum = 0;
      max = Math.max(max, sum);
    }
    System.out.println(max);
  }
}
