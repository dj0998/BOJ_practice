import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);

    int count = 0;
    String str = sc.next();
    int len = str.length();
    int t = sc.nextInt();
    
    for(int i = 0; i < t; i++) {
      String s = sc.next();
      s += s;
      if(s.contains(str)) count++;
    }
   
    System.out.println(count);
  }
}
