import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);

   int s = 0;
   int t = 0;
    
   for (int i = 0; i < 4; i++) {
    int a = sc.nextInt();
    s += a;
    }
    
    for (int i = 0; i < 4; i++){
     int b = sc.nextInt();
     t += b;
     } 

   if(s == t) System.out.print(s);
   else System.out.print(Math.max(s, t));

  } 
}
