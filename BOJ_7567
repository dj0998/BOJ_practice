import java.util.Scanner;
 
public class Main {
  public static void main(String[] args) {
    Scanner sc = new Scanner(System.in);

    int sum = 10;
    String arr = sc.next();

    for(int i = 1; i < arr.length(); i++) {
      if(arr.charAt(i-1) == arr.charAt(i)) sum += 5;
      else sum+= 10; 
    }
    System.out.println(sum);
  
  } 
}
