import java.util.Scanner;

public class Main {
  public static void main(String[] args) {
   Scanner sc = new Scanner(System.in);

    int t = sc.nextInt();
    
    for(int i = 0; i < t; i++) {
      int sum = 0;
      int n = sc.nextInt();
      for(int j = 0; j < n; j++) {
        int a = sc.nextInt();
        sum += a;
      }
      System.out.println(sum);
    }
  } 
}
